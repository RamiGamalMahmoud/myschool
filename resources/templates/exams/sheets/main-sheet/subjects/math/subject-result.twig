{% if entity.math.subjectResult.total.isAbsence %}
	{% include absence %}
{% else %}
	<td>{{entity.math.subjectResult.total.value}}</td>
{% endif %}

{% if entity.math.subjectResult.netDegree.isAbsence %}
	{% include absence %}
{% else %}
	<td>{{entity.math.subjectResult.netDegree.value}}</td>
{% endif %}

{% if entity.math.subjectResult.grade.isAbsence %}
	{% include absence %}
{% else %}
	{% if entity.math.subjectResult.grade.isPassed != true %}
		<td class="danger">{{entity.math.subjectResult.grade.value}}</td>
	{% else %}
		<td>{{entity.math.subjectResult.grade.value}}</td>
	{% endif %}
{% endif %}
