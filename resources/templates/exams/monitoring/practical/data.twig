{% import "exams/general/absence.twig" as absence %}

<td dataname="studentId" studentid="{{ entity.studentData.studentId }}">{{ serial }}</td>
<td dataname="sittingNumber">{{ entity.studentData.sittingNumber }}</td>
<td dataname="studentName">{{ entity.studentData.studentName }}</td>
<td dataname="classNumber">{{ entity.studentData.classNumber }}</td>

{% if entity.sciences.isAbsence %}
	{{ absence.absenceCell(entity.sciences.value, true) }}
{% else %}
	<td contenteditable="true">{{ entity.sciences.value }}</td>
{% endif %}

{% if entity.computer.isAbsence %}
	{{ absence.absenceCell(entity.computer.value, true) }}
{% else %}
	<td contenteditable="true">{{ entity.computer.value }}</td>
{% endif %}
