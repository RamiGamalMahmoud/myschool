<div class="section sheet">
	<div class="data-table fs-sheet">
		<div class="table-head">
			<table class="table">
				<thead>
					<tr class="head-space-holder">
						{% include "exams/fs-sheet/columns-sizes.twig" %}
					</tr>
					<tr>
						{% include "exams/fs-sheet/header/global-columns-names.twig" %}
					</tr>
					<tr>
						{% include "exams/fs-sheet/header/columns-names.twig" %}
					</tr>
					<tr>
						{% include "exams/fs-sheet/header/max-degs.twig" %}
					</tr>
					<tr>
						{% include "exams/fs-sheet/header/min-degs.twig" %}
					</tr>
				</thead>
			</table>
		</div>

		<div class="table-body">
			<table class="table">
				<thead>
					<tr class="head-space-holder">
						{% include "exams/fs-sheet/columns-sizes.twig" %}
					</tr>
				</thead>
				<tbody>
					{% set serial = 1 %}
					{% for entity in entities %}
						<tr>
							{% include "exams/fs-sheet/data.twig" %}
						</tr>
						{% set serial = serial + 1 %}
					{% endfor %}
				</tbody>
			</table>
		</div>
	</div>
</div>
