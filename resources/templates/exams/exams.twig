{% extends parentTemplate %}

{% block content %}

	<div
		class='content exams-content'>

		<!-- The Header of the page -->
		<div class='page-header'>
			<div class='menu drop-down-menu profile-menu'>
				<span class='menu-title avatar'>الكنترول</span>
				<div class="menu-content">
					<div class="menu-content-wrapper">
						<div class="menu drop-down-menu">
							<span class="menu-title">
								<i class="fa fa-user"></i>
								<span class="user-name">{{ fullName }}</span>
								<i class="fa fa-angle-down"></i>
							</span>
							<div class="menu-content">
								<div class="menu-content-wrapper">
									<a href="/logout" class="menu-btn">
										<i class="fa fa-sign-out"></i>
										تسجيل الخروج
									</a>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>

			<div class='menu-bar'>

				<div class='menu drop-down-menu'>
					<span class='menu-title'>الصف</span>
					<div class='menu-content'>
						<div class='menu-content-wrapper'>
							<a href='{{ linkPrefex }}/1' class='menu-btn'>الأول</a>
							<a href='{{ linkPrefex }}/2' class='menu-btn'>الثاني</a>
						</div>
					</div>
				</div>

				<div class='menu drop-down-menu'>
					<span class='menu-title'>رصد الدور الأول</span>
					<div class='menu-content'>
						<div class='menu-content-wrapper'>
							<div class='menu left-slide-menu'>
								<span class='menu-title'>التيرم الأول</span>
								<div class='menu-content'>
									<div class='menu-content-wrapper'>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/monitoring/evaluation/fs' class='menu-btn'>التقويمات</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/monitoring/practical/fs' class='menu-btn'>العملي</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/monitoring/written/fs' class='menu-btn'>التحريري</a>
									</div>
								</div>
							</div>

							<div class='menu left-slide-menu'>
								<span class='menu-title'>التيرم الثاني</span>
								<div class='menu-content'>
									<div class='menu-content-wrapper'>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/monitoring/evaluation/ss' class='menu-btn'>التقويمات</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/monitoring/practical/ss' class='menu-btn'>العملي</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/monitoring/written/ss' class='menu-btn'>التحريري</a>
									</div>
								</div>
							</div>
							<a href='' class='menu-btn'>تقارير الرصد</a>
						</div>
					</div>
				</div>

				<div class='menu drop-down-menu'>
					<span class='menu-title'>الشهادات</span>
					<div class='menu-content'>
						<div class='menu-content-wrapper'>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/certificates/ss' class='menu-btn'>نصف العام</a>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/certificates/ss' class='menu-btn'>آخر العام</a>
						</div>
					</div>
				</div>

				<div class='menu drop-down-menu'>
					<span class='menu-title'>الشيتات</span>
					<div class='menu-content'>
						<div class='menu-content-wrapper'>

							<div class="menu left-slide-menu">
								<span class="menu-title">نصف العام</span>
								<div class="menu-content">
									<div class="menu-content-wrapper">
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/sheets/fs/all' class='menu-btn' class='menu-btn'>كل الطلبة</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/sheets/fs/passed' class='menu-btn' class='menu-btn'>الناجحون</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/sheets/fs/failed' class='menu-btn' class='menu-btn'>الضعاف</a>
									</div>
								</div>
							</div>

							<div class="menu left-slide-menu">
								<span class="menu-title">آخر العام</span>
								<div class="menu-content">
									<div class="menu-content-wrapper">
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/sheets/ss/all' class='menu-btn'>كل الطلبة</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/sheets/ss/passed' class='menu-btn'>الناجحون</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/sheets/ss/failed' class='menu-btn'>لهم دور ثان</a>
									</div>
								</div>
							</div>

						</div>
					</div>
				</div>

				<div class='menu drop-down-menu'>
					<span class='menu-title'>كشوف</span>
					<div class='menu-content'>
						<div class='menu-content-wrapper'>
							<div class='menu left-slide-menu'>
								<span class='menu-title'>العشرة الأوائل</span>
								<div class='menu-content'>
									<div class='menu-content-wrapper'>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statements/top-ten/fs' class='menu-btn'>التيرم الأول</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statements/top-ten/ss' class='menu-btn'>آخر العام</a>
									</div>
								</div>
							</div>

							<div class='menu left-slide-menu'>
								<span class='menu-title'>نتيجة إجمالية</span>
								<div class='menu-content'>
									<div class='menu-content-wrapper'>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statements/overall-result/fs' class='menu-btn'>التيرم الأول</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statements/overall-result/ss' class='menu-btn'>آخر العام</a>
										<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statements/overall-result/second-round' class='menu-btn'>بعد الدور الثاني</a>
									</div>
								</div>
							</div>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statements/success' class='menu-btn'>ناجحون دور أول و ثان</a>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statements/failer' class='menu-btn'>راسب و باق للإعادة</a>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statements/mehany' class='menu-btn'>محول مهني</a>
						</div>
					</div>
				</div>

				<div class='menu drop-down-menu'>
					<span class='menu-title'>إحصاءات</span>
					<div class='menu-content'>
						<div class='menu-content-wrapper'>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statistics/fs' class='menu-btn'>التيرم الأول</a>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statistics/ss' class='menu-btn'>آخر العام</a>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}/statistics/second-round' class='menu-btn'>بعد الدور الثاني</a>
						</div>
					</div>
				</div>

				<div class='menu drop-down-menu'>
					<span class='menu-title'>الدور الثاني</span>
					<div class='menu-content'>
						<div class='menu-content-wrapper'>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}' class='menu-btn'>كشوف المناداة</a>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}' class='menu-btn'>دليل الدور الثاني</a>
							<a href='{{ linkPrefex }}/{{ gradeNumber ? : 1 }}' class='menu-btn'>توقيع الدور الثاني</a>
						</div>
					</div>
				</div>

				<div class='menu drop-down-menu' id='options'>
					<span class='menu-title'>خيارات</span>
					<div class="menu-content">
						<div class="menu-content-wrapper">
							<span class="menu-btn">إخفاء الألوان</span>
						</div>
					</div>
				</div>

			</div>
		</div>

		{% if child %}
			{% include template_from_string(child) %}
		{% endif %}
	</div>

{% endblock content %}
{% block js %}
	<script src="/js/exams/content.js" type="module"></script>
{% endblock js %}
